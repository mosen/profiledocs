{
    "$schema": "http://json-schema.org/schema#", 
    "type": "object", 
    "properties": {
        "PayloadUUID": {
            "default": "", 
            "pattern": "^[0-9A-Za-z]{8}-[0-9A-Za-z]{4}-[0-9A-Za-z]{4}-[0-9A-Za-z]{4}-[0-9A-Za-z]{12}$", 
            "type": "string", 
            "description": "Unique identifier for the payload (format 01234567-89AB-CDEF-0123-456789ABCDEF)"
        }, 
        "ProxyType": {
            "default": "Manual", 
            "enum": [
                "Manual", 
                "Auto"
            ], 
            "type": "string", 
            "description": "Proxy Type"
        }, 
        "PayloadDescription": {
            "default": "Global HTTP Proxy", 
            "type": "string", 
            "description": "Description of the payload"
        }, 
        "ProxyServerPort": {
            "type": "number", 
            "description": "Proxy Server Port"
        }, 
        "ProxyPassword": {
            "type": "string", 
            "description": "Proxy Password"
        }, 
        "ProxyPACFallbackAllowed": {
            "default": false, 
            "type": "boolean", 
            "description": "Proxy PAC Fallback Allowed"
        }, 
        "PayloadOrganization": {
            "type": "string", 
            "description": "This value describes the issuing organization of the profile, as displayed to the user"
        }, 
        "PayloadIdentifier": {
            "default": "com.apple.proxy.http.global", 
            "type": "string", 
            "description": "A unique identifier for the payload, dot-delimited.  Usually root PayloadIdentifier+subidentifier"
        }, 
        "PayloadDisplayName": {
            "default": "Global HTTP Proxy", 
            "type": "string", 
            "description": "Name of the payload"
        }, 
        "PayloadType": {
            "default": "com.apple.proxy.http.global", 
            "type": "string", 
            "description": "The type of the payload, a reverse dns string"
        }, 
        "ProxyServer": {
            "type": "string", 
            "description": "Proxy Server"
        }, 
        "ProxyCaptiveLoginAllowed": {
            "default": false, 
            "type": "boolean", 
            "description": "Proxy Bypass Allowed"
        }, 
        "ProxyPACURL": {
            "type": "string", 
            "description": "Proxy PAC URL"
        }, 
        "ProxyUsername": {
            "type": "string", 
            "description": "Proxy Username"
        }, 
        "PayloadVersion": {
            "default": 1, 
            "type": "number", 
            "description": "The version of the whole configuration profile."
        }
    }, 
    "title": "com.apple.proxy.http.global"
}