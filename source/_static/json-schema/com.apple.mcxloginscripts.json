{
    "$schema": "http://json-schema.org/schema#", 
    "type": "object", 
    "properties": {
        "PayloadUUID": {
            "default": "", 
            "pattern": "^[0-9A-Za-z]{8}-[0-9A-Za-z]{4}-[0-9A-Za-z]{4}-[0-9A-Za-z]{4}-[0-9A-Za-z]{12}$", 
            "type": "string", 
            "description": "Unique identifier for the payload (format 01234567-89AB-CDEF-0123-456789ABCDEF)"
        }, 
        "PayloadDescription": {
            "default": "Login scripts", 
            "type": "string", 
            "description": "Description of the payload"
        }, 
        "logoutscripts": {
            "type": "array", 
            "description": "Logout scripts"
        }, 
        "PayloadOrganization": {
            "type": "string", 
            "description": "This value describes the issuing organization of the profile, as displayed to the user"
        }, 
        "loginscripts": {
            "type": "array", 
            "description": "Login scripts"
        }, 
        "PayloadIdentifier": {
            "default": "com.apple.mcxloginscripts", 
            "type": "string", 
            "description": "A unique identifier for the payload, dot-delimited.  Usually root PayloadIdentifier+subidentifier"
        }, 
        "PayloadDisplayName": {
            "default": "Login scripts", 
            "type": "string", 
            "description": "Name of the payload"
        }, 
        "PayloadType": {
            "default": "com.apple.mcxloginscripts", 
            "type": "string", 
            "description": "The type of the payload, a reverse dns string"
        }, 
        "skipLoginHook": {
            "type": "boolean", 
            "description": "Disable or enable the LoginHook. Must be false to use login scripts"
        }, 
        "PayloadVersion": {
            "default": 1, 
            "type": "number", 
            "description": "The version of the whole configuration profile."
        }, 
        "skipLogoutHook": {
            "type": "boolean", 
            "description": "Disable or enable the LogoutHook. Must be false to use logout scripts"
        }
    }, 
    "title": "com.apple.mcxloginscripts"
}